#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/jaimeisraelcampeas/keystone/packages/core/bin/node_modules:/Users/jaimeisraelcampeas/keystone/packages/core/node_modules:/Users/jaimeisraelcampeas/keystone/packages/node_modules:/Users/jaimeisraelcampeas/keystone/node_modules:/Users/jaimeisraelcampeas/node_modules:/Users/node_modules:/node_modules:/Users/jaimeisraelcampeas/keystone/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/jaimeisraelcampeas/keystone/packages/core/bin/node_modules:/Users/jaimeisraelcampeas/keystone/packages/core/node_modules:/Users/jaimeisraelcampeas/keystone/packages/node_modules:/Users/jaimeisraelcampeas/keystone/node_modules:/Users/jaimeisraelcampeas/node_modules:/Users/node_modules:/node_modules:/Users/jaimeisraelcampeas/keystone/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../packages/core/bin/cli.js" "$@"
else
  exec node  "$basedir/../../../../packages/core/bin/cli.js" "$@"
fi
